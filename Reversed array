package com.gmail.orfejka;

import java.util.Arrays;

public class Main {

	public static void main(String[] args) {

		int mass[] = { 5, 5, 1, 12, 23, 4, 6, 5 };
		for (int i = 0; i < mass.length / 2; i++) {
			int j = mass.length - i - 1;
			int a = mass[i];
			mass[i] = mass[j];
			mass[j] = a;
		}
		System.out.println(Arrays.toString(mass));

	}

}
